{
    "manifestVersion": 1,
    "id": "WaitForUrls",
    "name": "Wait for URLs",
    "version": "1.0.1",
    "publisher": "alexplaza",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "This task is used to verify that a given list of sites are up and running.",
    "categories": [
        "Build and release"
    ],
    "icons": {
        "default": "extension-icon.png"
    },
    "files": [
        {
            "path": "waitTask", "addressable": true
        }
    ],
    "contributions": [
       {
           "id": "wait-task",
           "type": "ms.vss-distributed-task.task",
           "targets": [
               "ms.vss-distributed-task.tasks"
           ],
           "properties": {
               "name": "waitTask"
           }
       }
   ]
}
